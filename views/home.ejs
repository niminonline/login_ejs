
<%- include('header') %>


<h2 class="text-center text-white my-5"> Home Page</h2>
<div class="card-block container-fluid d-flex justify-content-center row">
<% data.forEach(element => { %>
    <div class="card col-lg-3 col-md-6 col-sm-12 m-3" style="width: 18rem;">
        <img class="card-img-top" src="<%=element.image%>" alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title"><%=element.title%></h5>
          <p class="card-text"><%=element.description%></p>
          <a href="#" class="btn btn-primary"><%=element.btnText%></a>
        </div>
      </div>
   
<%});%>
</div>




<%- include('footer') %>

